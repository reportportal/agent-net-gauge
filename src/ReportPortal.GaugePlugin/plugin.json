{
  "id": "reportportal",
  "version": "1.9.0",
  "name": "ReportPortal",
  "description": "Sends specification execution results to http://ReportPortal.io",
  "PostInstall": {
    "windows": [],
    "linux": [ "chmod", "+x", "ReportPortal.GaugePlugin" ],
    "darwin": [ "chmod", "+x", "ReportPortal.GaugePlugin" ]
  },
  "command": {
    "windows": [ "ReportPortal.GaugePlugin.exe" ],
    "linux": [ "./ReportPortal.GaugePlugin" ],
    "darwin": [ "./ReportPortal.GaugePlugin" ]
  },
  "scope": [ "Execution" ],
  "gaugeVersionSupport": {
    "minimum": "1.0.7",
    "maximum": ""
  },
  "capabilities": [
    "grpc_support"
  ]
}